<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bluegelap"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".SignupActivity">



            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="412dp"
                android:layout_height="706dp"
                android:layout_marginTop="148dp"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/kotakbawah"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="-1dp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="53dp"
                android:layout_height="50dp"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="21dp"
                android:src="@drawable/kotakkecil"
                app:layout_constraintStart_toEndOf="@+id/password"
                app:layout_constraintTop_toBottomOf="@+id/email" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="7dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="22dp"
                android:src="@drawable/mata"
                app:layout_constraintStart_toEndOf="@+id/password"
                app:layout_constraintTop_toBottomOf="@+id/email" />

            <ImageView
                android:id="@+id/kembali1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginStart="35dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="52dp"
                android:clickable="true"
                android:onClick="pindahHomeActivity"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/tombolbalik" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="55dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="111dp"
                android:fontFamily="@font/poppins_bold"
                android:text="GET STARTED"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/password"
                android:layout_width="230dp"
                android:layout_height="50dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="21dp"
                android:background="@drawable/kotaktext"
                android:ems="10"
                android:fontFamily="@font/poppins_bold"
                android:hint="Password"
                android:inputType="textPassword"
                android:paddingLeft="18dp"
                android:singleLine="false"
                android:textColor="#A7A7A7"
                android:textColorHint="#A7A7A7"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/email" />

            <EditText
                android:id="@+id/email"
                android:layout_width="287dp"
                android:layout_height="50dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="93dp"
                android:background="@drawable/kotaktext"
                android:ems="10"
                android:fontFamily="@font/poppins_bold"
                android:hint="Email"
                android:inputType="textPersonName"
                android:paddingLeft="18dp"
                android:singleLine="false"
                android:textColor="#A7A7A7"
                android:textColorHint="#A7A7A7"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <EditText
                android:id="@+id/Username"
                android:layout_width="287dp"
                android:layout_height="50dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="21dp"
                android:background="@drawable/kotaktext"
                android:ems="10"
                android:fontFamily="@font/poppins_bold"
                android:hint="Username"
                android:inputType="textPersonName"
                android:paddingLeft="18dp"
                android:singleLine="false"
                android:textColor="#A7A7A7"
                android:textColorHint="#A7A7A7"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/password" />

            <EditText
                android:id="@+id/kelas"
                android:layout_width="287dp"
                android:layout_height="50dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="21dp"
                android:background="@drawable/kotaktext"
                android:ems="10"
                android:fontFamily="@font/poppins_bold"
                android:hint="Kelas"
                android:inputType="textPersonName"
                android:paddingLeft="18dp"
                android:singleLine="false"
                android:textColor="#A7A7A7"
                android:textColorHint="#A7A7A7"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/NIM" />

            <EditText
                android:id="@+id/NIM"
                android:layout_width="287dp"
                android:layout_height="50dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="21dp"
                android:background="@drawable/kotaktext"
                android:ems="10"
                android:fontFamily="@font/poppins_bold"
                android:hint="NIM"
                android:inputType="textPersonName"
                android:paddingLeft="18dp"
                android:singleLine="false"
                android:textColor="#A7A7A7"
                android:textColorHint="#A7A7A7"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Username" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="248dp"
                android:layout_height="42dp"
                android:layout_marginStart="55dp"
                android:layout_marginLeft="55dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Masukkan data diri Anda Sesuai dengan data Anda sebenarnya."
                android:textColor="#B9B9B9"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <ImageView
                android:id="@+id/google"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:layout_marginStart="99dp"
                android:layout_marginLeft="99dp"
                android:layout_marginTop="13dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                app:srcCompat="@drawable/google" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="96dp"
                android:layout_height="18dp"
                android:layout_marginStart="158dp"
                android:layout_marginLeft="158dp"
                android:layout_marginTop="13dp"
                android:fontFamily="@font/poppins_bold"
                android:text="or sign up with"
                android:textColor="#B9B9B9"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/signup" />

            <ImageView
                android:id="@+id/apple"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="13dp"
                app:layout_constraintStart_toEndOf="@+id/google"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                app:srcCompat="@drawable/apple" />

            <ImageView
                android:id="@+id/github"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="13dp"
                app:layout_constraintStart_toEndOf="@+id/apple"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                app:srcCompat="@drawable/hackerrank" />

                <Button
                    android:id="@+id/signup"
                    android:layout_width="287dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="62dp"
                    android:layout_marginLeft="62dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/kotakkeciltext"
                    android:fontFamily="@font/poppins_bold"
                    android:text="SIGN UP"
                    android:textColor="#FFFFFF"
                    app:backgroundTint="#2F353F"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/kelas" />

</androidx.constraintlayout.widget.ConstraintLayout>
<!--</LinearLayout>-->